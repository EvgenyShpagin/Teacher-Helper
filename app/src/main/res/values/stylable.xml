<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ListItemView">
        <attr name="position" format="enum">
            <enum name="first" value="0" />
            <enum name="middle" value="1" />
            <enum name="last" value="2" />
            <enum name="single" value="3" />
        </attr>
        <attr name="title" format="string" />
        <attr name="leadingComponent" format="enum">
            <enum name="checkbox" value="0" />
            <enum name="radioButton" value="1" />
            <enum name="icon" value="2" />
            <enum name="nothing" value="3" />
        </attr>
        <attr name="trailingComponent" format="enum">
            <enum name="icon" value="2" />
            <enum name="nothing" value="3" />
            <enum name="supportText" value="4" />
            <enum name="switch1" value="5" />
        </attr>
        <attr name="isChecked" format="boolean" />
        <attr name="leadingDrawable" format="reference" />
        <attr name="trailingDrawable" format="reference" />
        <attr name="trailingSupportText" format="string" />
        <attr name="isErrorColor" format="boolean" />
        <attr name="isEnabled" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ListLayout">
        <attr name="label" format="string" />
    </declare-styleable>
</resources>